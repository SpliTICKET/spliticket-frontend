{
	"root": true,
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["@vue/typescript/recommended", "eslint:recommended", "plugin:vue/vue3-recommended", "@vue/prettier"],
	"parserOptions": {
		"ecmaVersion": 2021
	},
	"rules": {
		"no-console": "error",
		"no-debugger": "error",
		"@typescript-eslint/interface-name-prefix": "off",
		"@typescript-eslint/explicit-function-return-type": "off",
		"@typescript-eslint/explicit-module-boundary-types": "off",
		"@typescript-eslint/no-explicit-any": "off",
		"@typescript-eslint/no-non-null-assertion": "off",
		"@typescript-eslint/no-unused-vars": [
			"off",
			{
				"argsIgnorePattern": "^_"
			}
		],
		"prettier/prettier": [
			"error",
			{},
			{
				"usePrettierrc": true
			}
		]
	}
}
